	$transparent: rgba(#FFFFFF, 0.85);
	$clear: rgba(#FFFFFF, 0);
	$logo-font: "Kaushan Script", cursive;
	$white       : #FFFFFF;
	$oil         : #333333;
	$jet         : #222222;
	$dark-blue: #001829;
	$primary-blue: #0060A1;
	$green: #00A141;
	$bg-grey: #fcfcfc;
	$alert-color: #F22C00;

@mixin full-height(){
    height: 100%;
  
}

html, body{
	@include full-height();
}

#navbar{
	z-index: 9999;
}
#top-overlay li {
  background: $clear; 
}
.logo, #logo{
	font-family: $logo-font;
	color: $primary-blue;
}
.logo:hover, #logo:hover{
	color: $alert-color;
}

.background{
	@include full-height();
	// background-color: $primary-blue;
	background-image: image-url('header.svg'), linear-gradient(to bottom, rgba(0,96,161,0.57) 0%, rgba(73,155,234,1) 100%), image-url('womens-fitness.jpg');
	-webkit-background-size: cover;
  	-moz-background-size: cover;
  	-o-background-size: cover;
  	background-size: cover;
	width: 100%;
	z-index: 1;
	margin-top: -85px


}


#jumbotron{
	position: relative;
	top: -43%;

	//;

}

.jumbo{

	
	width: 100%;
}


