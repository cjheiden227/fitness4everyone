<section class="text-shadow background" id="contact">
	<div class="row">
		<div class="small-12 medium-centered medium-10 columns">
			<h1 class="text-center">Get in Touch, <br> Schedule a Consultation,<br> Get an Appointment! </h1>
		</div>
	</div>
	<form>
	  <div class="row">
	    <div class="small-12 medium-centered medium-10 columns">
	      <label>Name
	        <input type="text" placeholder="large-12.columns" />
	      </label>
	    </div>
	  </div>
	  <div class="row">
	    <div class="small-12 medium-offset-1 medium-5 columns">
	      <label>Phone Number
	        <input type="text" placeholder="large-4.columns" />
	      </label>
	    </div>
	    <div class="small-12 medium-5 end columns">
	      <label>Email
	        <input type="text" placeholder="large-4.columns" />
	      </label>
	    </div>

	  </div>
	  
	  <div class="row">
	    <div class="small-12 medium-10 small-centered columns">
	      <label>Message
	        <textarea placeholder="small-12.columns"></textarea>
	      </label>
	    </div>
	  </div>
	</form>
</section>